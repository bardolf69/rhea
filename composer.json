{
    "name": "lara-zeus/rhea",
    "description": "migrate your wordpress blog to zeus sky",
    "keywords": [
        "laravel",
        "lara-zeus",
        "sky",
        "cms",
        "pages",
        "posts",
        "menu",
        "news",
        "wordpress laravel",
        "wordpress migrate",
        "wordpress alternative",
        "laravel cms",
        "filamentphp"
    ],
    "homepage": "https://larazeus.com/rhea",
    "support": {
        "issues": "https://github.com/lara-zeus/rhea/issues",
        "source": "https://github.com/lara-zeus/rhea"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "php coder",
            "email": "info@larazeus.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.1",
        "jgrossi/corcel": "^7.0 || ^8.0",
        "lara-zeus/sky": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Rhea\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Rhea\\RheaServiceProvider"
            ],
            "aliases": {
                "Rhea": "LaraZeus\\Rhea\\RheaFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
